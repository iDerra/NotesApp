{
  "expo": {
    "name": "Notes",
    "slug": "notesApp",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/notes-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iderra.notesapp"
    },
    "android": {
      "package": "com.iderra.notesapp",
      "versionCode": 1,
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "label": "Notes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/notes-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/notes-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/notes-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "color": "white"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ec4658ce-178f-4270-b7fa-f07a31059c62"
      }
    }
  }
}
